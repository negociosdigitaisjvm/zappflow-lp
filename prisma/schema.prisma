generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Image {
  id        String   @id @default(uuid())
  name      String
  path      String
  url       String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
